Algoritmo cambio_variables_a_usuario
	
	///variables
	Definir valor1, valor2, aux Como Entero
	
	///ingresar
	escribir "ingrese el primer valor entero"
	Leer valor1
	
	Escribir "ingrese el segundo valor entero"
	Leer valor2
	
	///auxiliar
	aux = valor1
	valor1 = valor2
	valor2 = aux
	
	///respuesta
	Escribir "el valor de sus variables es = "  ,valor1, " " , valor2 ;
	
	
	
	
FinAlgoritmo

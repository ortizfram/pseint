Algoritmo suma_resta_multiplic_div
	///variables
	definir val1, val2 Como Real
	
	///ingresar valores
	Escribir "ingrese el valor entero 1"
	Leer val1
	Escribir "ingrese el valor entero 2"
	Leer val2
	
	///operaciones
	Escribir "**TUS RESULTADOS SON**"
	Escribir "suma = " (val1 + val2 )
	Escribir "resta = " (val1 - val2 )
	Escribir "multiplicación = " (val1 * val2 )
	Escribir "división = " (val1 / val2 )
	
	
FinAlgoritmo
